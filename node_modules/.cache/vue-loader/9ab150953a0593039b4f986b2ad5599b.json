      </form>\r\n          </div>\r\n         \r\n        </b-modal>\r\n         <div class=\"row\"  style=\"padding-bottom:10px\" >\r\n           <div class=\"col\" style=\"padding-left:30px\">\r\n             <div class=\"row\">\r\n              <div class=\"col-xs-2\">\r\n                <button class=\"btn btn-danger right\" id=\"show-btn\" @click=\"$bvModal.show('bv-modal-example-wajib')\"><font-awesome-icon icon=\"minus\" /> Hapus</button>\r\n              </div>\r\n              <div class=\"col-xs-2\">\r\n                <button class=\"btn btn-primary right button\" id=\"show-btn\" @click=\"$bvModal.show('bv-modal-example-wajib')\"><font-awesome-icon icon=\"plus\" /> Tambah</button>\r\n              </div>\r\n             </div>\r\n           </div>\r\n           <div class=\"col\" style=\"margin-left:47%\">\r\n             <div class=\"row\" >\r\n                 <div class=\"col-lg-9\">\r\n                <input class=\"form-control\"  id=\"ex1\" type=\"text\">\r\n              </div>\r\n              <div class=\"col\">\r\n                <button class=\"btn btn-primary button\" id=\"show-btn\"><font-awesome-icon :icon=\"['fas', 'search']\" class=\"icon alt\"/></button>\r\n              </div>\r\n             </div>\r\n           </div>\r\n        </div>\r\n\r\n        <table class=\"table table-bordered\" style=\"table-layout: fixed\">\r\n            <thead>\r\n            <tr >\r\n                <th style=\"text-align:center\">Hapus</th>\r\n                <th>Id KTA</th>\r\n                <th>Penyetor</th>\r\n                <th>Tanggal setoran</th>\r\n                <th>Jumlah yang disetor</th>\r\n                <th>Total setoran</th>\r\n                <th style=\"text-align:center\">Edit</th>\r\n            </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"pokok in simpanan\" :key=\"pokok.jenis_simpanan==='wajib'\" >\r\n                    <td><center><input type=\"checkbox\" value=\"delete\"></center></td>\r\n